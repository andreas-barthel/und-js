{
  "name": "@unification-com/und-js",
  "version": "1.8.2",
  "description": "Unification Mainchain Wallet SDK",
  "main": "lib/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --verbose",
    "build": "rm -rf lib && npx babel src -d lib",
    "build:docs": "npx jsdoc-to-markdown -f src/**/*.js > docs/jsdoc.md",
    "prepublishOnly": "npm run build",
    "lint": "eslint --fix src __tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unification-com/und-js.git"
  },
  "keywords": [
    "FUND",
    "Unification"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/unification-com/und-js/issues"
  },
  "homepage": "https://github.com/unification-com/und-js#readme",
  "dependencies": {
    "@ledgerhq/hw-transport": "^5.21.0",
    "@ledgerhq/hw-transport-u2f": "^5.21.0",
    "@ledgerhq/hw-transport-webusb": "^5.21.0",
    "axios": "^0.19.2",
    "bech32": "^1.1.4",
    "big.js": "^5.2.2",
    "bip32": "^1",
    "bip39": "^3.0.2",
    "bn.js": "^4.11.9",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^3.3.0",
    "is_js": "^0.9.0",
    "ledger-cosmos-js": "^2.1.7",
    "safe-buffer": "^5.2.1",
    "secp256k1": "^4.0.2",
    "secure-random": "^1.1.2",
    "tiny-secp256k1": "^1.1.5",
    "uuid": "^3.4.0",
    "varstruct": "^6.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@ledgerhq/hw-transport-node-hid": "^5.21.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.11.1",
    "jest": "^25.5.4",
    "jsdoc-to-markdown": "^5.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-async-to-generator",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  },
  "files": [
    "lib/*"
  ],
  "resolutions": {
    "**/**/handlebars": "^4.1.2",
    "**/**/ecstatic": "^3.3.2",
    "**/**/mem": "^4.0.0"
  }
}
